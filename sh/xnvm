alias @nvm_install="storecmd 'cmd@curl -o- https://raw.githubusercontent.com/nvm-sh/nvm/v0.37.0/install.sh | bash'"
alias @nvm="storecmd 'cmd@nvm install 12' \
                     'cmd@nvm alias default 12' \
                     'cmd@nvm use 12' \
                     'cmd@nvm ls' \
                     'cmd@nvm ls-remote' \
                     'cmd@nvm reinstall-packages 12'"
