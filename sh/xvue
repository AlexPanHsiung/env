alias @vueproj="storecmd 'cmd@npm install -g @vue/cli' \
		         'cmd@vue create vueproj'  \
		         'cmd@vue add bootstrap-vue'  \
		         'cmd@vue add element'"

alias @vue-element="rm -rf vuepro;mkdir vuepro;cd vuepro;
                        cp -a ~/.env/tpl/vue-element/* .;
                        echo 'create vuepro element'"
