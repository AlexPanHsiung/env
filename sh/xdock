alias xdockBuild="storecmd 'cmd@docker build -t \$1_mybuntu .'"
alias xdockRun="storecmd 'cmd@docker images' \
			 'cmd@docker run -it -v $PWD/$USER:/home/$USER \$1_REPOSITORY /bin/bash'"

alias xdockImage="storecmd 'cmd@docker images'"
alias xdockPs="storecmd 'cmd@docker ps -a'"
alias xdockNetwork="storecmd 'cmd@docker network ls'"

alias xdockDelAllImgs="storecmd 'cmd@docker images -q | xargs docker rmi -f'"

alias xdockStackDeploy="storecmd 'cmd@docker swarm init' \
				 'cmd@docker stack deploy -c docker-compose.yml \$1_my'"
alias xdockCommit="storecmd 'cmd@docker ps -a' \
			    'cmd@docker commit -m \$1_mypro ContianerID  \$1_mypro/proj:1.0'"


alias xdockExportImport="storecmd 'cmd@docker ps -a' \
				  'cmd@docker import \$1_ContainerID myimag.tar' \
				  'cmd@docker export \$1_ContainerID > myimag.tar'"


