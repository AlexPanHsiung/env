alias xgitBranch="storecmd 'git branch'"
alias xgitBranchAdd="storecmd 'git branch \$1_addBranch'"
alias xgitBranchDel="storecmd 'git branch -d \$1_delBranch'"
alias xgitBranchNewCheckout="storecmd 'git checkout -b \$1'"

alias xgitComm="storecmd 'git tag \$1_tag100' 'git commit -m \"add main\"'"
alias xgitCommBranch="storecmd 'git tag \$1_tag100' 'git commit -a -m \"add main\"'"

alias xgitCheckoutSwitch="storecmd 'git branch' 'git checkout \$1_mybranch'"
alias xgitCheckoutTag="storecmd 'git checkout tags/\$1_mytags'"
alias xgitMerge="storecmd 'git branch' 'git checkout \$1_mybranch' 'git merge \$1_otherBranch'"
