alias xcargoproj="rm -rf myproj; mkdir myproj;cd myproj;
		  cp -a ~/.env/tpl/proj/* .;"

alias xcargo_wasm-nodejs="rm -rf mynodejs; mkdir mynodejs; cd mynodejs;
			cp -a ~/.env/tpl/wasm-pack/notpl/* .;
			echo 'create wasm-nodejs project';
			"

alias xcargo_wasm-nodejs2="rm -rf mynodejs; mkdir mynodejs;cd mynodejs;
			cp -a ~/.env/tpl/wasm-pack/notpl2/* .;
			echo 'create wasm-nodejs2 project';
			"

