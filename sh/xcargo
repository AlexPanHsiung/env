alias xcargo="storecmd  'cmd@rustc --print target-list'  \
			'cmd@rustup target add wasm32-unknown-unknown' \
			'cmd@cargo new --bin|lib myproj' \
			'cmd@cargo build --target wasm32-unknown-unknown --release'"

alias xcargoTest="storecmd 'run@cargo test -- --nocapture'"

alias xcargo_wasm-wasmtime="cargo new mywasmtime; cd mywasmtime;
			cp -f ~/.env/tpl/wasmtime/CargoWasm.toml Cargo.toml;
			cp -f ~/.env/tpl/wasmtime/main.rs src/main.rs;
			cp -f ~/.env/tpl/wasmtime/build.sh .;
			cargo new --lib mylib;cd mylib;	
			cp -f ~/.env/tpl/wasmtime/CargoWasmtime.toml Cargo.toml;
			cp -f ~/.env/tpl/wasmtime/lib.rs src/lib.rs;
			cp -f ~/.env/tpl/wasmtime/build2.sh build.sh;
			cd ..;
			echo 'create wasmtime project';
		   "

alias xcargo_wasm-pack="rm -rf mywasmpack; mkdir mywasmpack; cd mywasmpack;
			cp -a ~/.env/tpl/wasm-pack/tpl/crate .;
			cp ~/.env/tpl/wasm-pack/tpl2/crate/index.js .;
		  	cp -f ~/.env/tpl/wasm-pack/tpl/index.js .;
		  	cp -f ~/.env/tpl/wasm-pack/tpl/index.html .;
		  	cp -f ~/.env/tpl/wasm-pack/tpl/webpack.config.js .;
		  	cp -f ~/.env/tpl/wasm-pack/tpl/package.json .;
		  	cp -f ~/.env/tpl/wasm-pack/tpl/build.sh .;
			echo 'create wasm-pack project';
			"

alias xcargo_wasm-pack2="rm -rf mywasmpack; mkdir mywasmpack; cd mywasmpack;
			cp -a ~/.env/tpl/wasm-pack/tpl2/crate .;
			cp ~/.env/tpl/wasm-pack/tpl2/crate/index.js .;
		  	cp -f ~/.env/tpl/wasm-pack/tpl2/index.js .;
		  	cp -f ~/.env/tpl/wasm-pack/tpl2/index.html .;
		  	cp -f ~/.env/tpl/wasm-pack/tpl2/webpack.config.js .;
		  	cp -f ~/.env/tpl/wasm-pack/tpl2/package.json .;
		  	cp -f ~/.env/tpl/wasm-pack/tpl2/build.sh .;
			echo 'create wasm-pack2 project';
			"
